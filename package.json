        "react-spring": "^8.0.7",
        "react-windowed-select": "0.0.3-alpha"
    },
    "devDependencies": {
        "@babel/core": "^7.2.0",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/preset-env": "^7.2.0",
        "@babel/preset-react": "^7.0.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.0.4",
